{
  "expo": {
    "name": "Mubeen - Quran Summaries",
    "slug": "mubeen",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "com.navedislam.mubeen",
      "versionCode": 6,
      "permissions": []
